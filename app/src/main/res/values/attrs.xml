<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppBarTranslateScaleBehavior">
        <attr name="behavior_destination_id" format="reference" />
    </declare-styleable>

    <declare-styleable name="DrawerBehavior">
        <attr name="android:layout_gravity"/>
    </declare-styleable>

    <declare-styleable name="BottomSheetCustomBehavior">
        <attr name="behavior_half_expanded_space" format="dimension"/>
        <attr name="behavior_full_expanded_space" format="dimension"/>
        <attr name="behavior_below_to" format="reference"/>
    </declare-styleable>

    <declare-styleable name="FaceLoadingView">
        <attr name="loading_image" format="reference"/>
    </declare-styleable>
</resources>